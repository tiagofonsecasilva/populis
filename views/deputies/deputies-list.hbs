    {{name}}
  </a>
  <br><br>
  <h3> Name:</h3><a href="/deputies/{{party}}">{{party}}</a>
  <br><br>
  <h3> Name:</h3>
  <form action="/deputies/{{_id}}/delete" method="POST">
